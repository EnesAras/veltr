{
  "name": "veltr",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "backend"
  ],
  "scripts": {
    "dev:storefront": "npm --prefix apps/storefront run dev",
    "dev:admin": "npm --prefix apps/admin run dev",
    "dev:backend": "npm --prefix backend run dev",
    "dev": "npm-run-all --parallel dev:storefront dev:admin dev:backend"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
